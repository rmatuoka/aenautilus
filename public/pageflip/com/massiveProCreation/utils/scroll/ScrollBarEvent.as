package com.massiveProCreation.utils.scroll{	import flash.events.*;		public class ScrollBarEvent extends Event	{		public static const VALUE_CHANGED = "valueChanged";		public var scrollPercent:Number;				public function ScrollBarEvent(sp:Number):void		{			// this class is a custom class which holds the events for the scroll bar			super(VALUE_CHANGED);			scrollPercent = sp;		}			}}